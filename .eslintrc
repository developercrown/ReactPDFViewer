{
  "parser": "babel-eslint",
  "settings": {
    "import/parser": "babel-eslint"
  },
  "rules": {
    "brace-style": [
      2,
      "1tbs",
      {
        "allowSingleLine": true
      }
    ],
    "camelcase": 2,
    "comma-dangle": [
      2,
      "never"
    ],
    "comma-spacing": [
      2,
      {
        "before": false,
        "after": true
      }
    ],
    "comma-style": [
      2,
      "last"
    ],
    "consistent-this": [
      2,
      "self"
    ],
    "curly": [
      2,
      "multi-line"
    ],
    "eqeqeq": 2,
    "guard-for-in": 0,
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "key-spacing": [
      2,
      {
        "beforeColon": false,
        "afterColon": true
      }
    ],
    "arrow-spacing": 2,
    "new-cap": [
      2,
      {
        "capIsNew": false,
        "newIsCap": true
      }
    ],
    "new-parens": 0,
    "no-bitwise": 0,
    "no-cond-assign": 2,
    "no-console": 0,
    "no-alert": 2,
    "no-constant-condition": 1,
    "no-delete-var": 2,
    "no-debugger": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty-character-class": 2,
    "no-eval": 2,
    "no-ex-assign": 2,
    "no-extend-native": 2,
    "no-floating-decimal": 2,
    "no-func-assign": 0,
    "no-implied-eval": 2,
    "no-inner-declarations": 2,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-iterator": 2,
    "no-labels": 0,
    "no-lonely-if": 2,
    "no-loop-func": 2,
    "no-mixed-operators": 0,
    "no-mixed-spaces-and-tabs": 2,
    "no-native-reassign": 2,
    "no-negated-in-lhs": 2,
    "no-new-func": 2,
    "no-new-object": 2,
    "no-new-wrappers": 2,
    "no-obj-calls": 2,
    "no-octal": 2,
    "no-plusplus": 0,
    "no-proto": 2,
    "no-redeclare": 2,
    "no-return-assign": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-shadow": 0,
    "no-spaced-func": 2,
    "no-sparse-arrays": 2,
    "no-throw-literal": 2,
    "no-trailing-spaces": 0,
    "no-unused-vars": [
      2,
      {
        "vars": "all",
        "args": "none"
      }
    ],
    "no-undef": 0, // replaced by "jsx-control-statements/jsx-jcs-no-undef"
    "no-undefined": 0,
    "no-undef-init": 2,
    "no-underscore-dangle": 0,
    "no-unexpected-multiline": 2,
    "no-unreachable": 2,
    "no-use-before-define": [
      2,
      "nofunc"
    ],
    "no-var": 2,
    "no-warning-comments": [
      0,
      {
        "terms": [
          "todo",
          "fixme"
        ]
      }
    ],
    "no-extra-parens": 0,
    "max-len": [
      2,
      200,
      2
    ],
    "quotes": [
      2,
      "single",
      "avoid-escape"
    ],
    "quote-props": [
      2,
      "consistent-as-needed"
    ],
    "radix": 2,
    "semi": [
      2,
      "always"
    ],
    "semi-spacing": [
      2,
      {
        "before": false,
        "after": true
      }
    ],
    "keyword-spacing": [
      0
    ],
    "space-before-blocks": [
      2,
      "always"
    ],
    "space-before-function-paren": [
      2,
      "never"
    ],
    "space-infix-ops": 2,
    "object-curly-spacing": [
      2,
      "always",
      {
        "arraysInObjects": true
      }
    ],
    "array-bracket-spacing": [
      2,
      "never"
    ],
    "computed-property-spacing": [
      2,
      "never"
    ],
    "space-in-parens": [
      2,
      "never"
    ],
    "space-unary-ops": [
      2,
      {
        "words": true,
        "nonwords": false
      }
    ],
    "strict": [
      0,
      "never"
    ],
    "use-isnan": 2,
    "valid-typeof": 2,
    "vars-on-top": 0, // disabled for now, see github.com/eslint/eslint/issues/2099
    "wrap-iife": [
      2,
      "inside"
    ],
    "eol-last": 0,
    "yoda": 0,
    // Node.js specific
    "handle-callback-err": [
      2,
      "^(err|error)$"
    ],
    "no-mixed-requires": 2,
    "no-new-require": 2,
    "no-path-concat": 2,
    // React
    // Not needed. "react/display-name": 2,
    "react/jsx-boolean-value": 2,
    "react/jsx-curly-spacing": [
      2,
      "never"
    ],
    "react/jsx-equals-spacing": [
      2,
      "never"
    ],
    "react/jsx-tag-spacing": [2, {
      "closingSlash": "never",
      "beforeSelfClosing": "always",
      "afterOpening": "never"
    }],
    "react/jsx-quotes": 0,
    "jsx-quotes": [
      2,
      "prefer-double"
    ],
    "react/jsx-no-undef": 2,
    "react/jsx-sort-props": 0,
    "react/jsx-sort-prop-types": 0,
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/jsx-indent": [
      2,
      2
    ],
    "react/jsx-indent-props": [
      2,
      2
    ],
    "react/no-did-mount-set-state": 2,
    "react/no-did-update-set-state": 2,
    "react/no-multi-comp": 0,
    "react/no-unknown-property": 2,
    "react/prop-types": 0,
    "react/react-in-jsx-scope": 2,
    "react/self-closing-comp": 2,
    // Not ready yet. "react/sort-comp": 2,
    "react/jsx-wrap-multilines": 2,
    "import/no-unresolved": [
      2,
      {
        "commonjs": true,
        "amd": true
      }
    ],
    "import/named": 2,
    "import/first": 2,
    "import/no-duplicates": 2,
    "import/newline-after-import": 2,
    "import/namespace": 2,
    "import/default": 0,
    "import/export": 2,
    "import/no-namespace": 0,
    "arrow-parens": [
      0,
      "as-needed"
    ]
  },
  "plugins": [
    "react",
    "import",
    "babel"
  ],
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "Promise": true,
    "Map": true,
    "Set": true,
    "process": true
  },
  "ecmaFeatures": {
    "jsx": true
  }
}
